resource_name = "c2_m2-functionality"
script/source = "extends Node3D

@onready var area = $Pain

@export var player_floor_damage = 10
@export var player_fling_velocity = 50
@export var enemy_floor_damage = 9999
@export var prop_fling_velocity = 50

#flor

func _on_pain_body_entered(body):
	print(body)
	#if body is RigidBody3D:
	#	body.apply_force(Vector3(0, prop_fling_velocity, 0))
	
	
	if body.is_in_group(\"player\"):
		body.velocity.y = player_fling_velocity
		
		for i in body.get_children():
			if i.is_in_group(\"info\"):
				i.Damage(player_floor_damage)
	
	
	if body.is_in_group(\"enemy\"):
		for i in body.get_children():
			if i.is_in_group(\"info\"):
				i.Damage(enemy_floor_damage) #kill enemies instantly if they touchie
	

"

