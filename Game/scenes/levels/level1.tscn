[sub_resource type="GDScript" id="GDScript_jf0va"]
resource_name = "level1_functionality"
script/source = "extends Node3D

@onready var player = $Player
@onready var enemy1 = $EnemySpawnPoint1
@onready var enemy2 = $EnemySpawnPoint2
@onready var level = $\"NavigationRegion3D/level\"

@export var enemy_type: PackedScene


func _ready():
	print_rich(\"[font_size=120][color=CORNFLOWER_BLUE][wave]heck you\")
	
	$Player.sendHintToPlayer(\"[wave][center][color=SILVER]Use MOUSE 1 to pick up and throw objects\")
	
	for i in range(level.get_children().size()):
		if level.get_children()[i] is RigidBody3D:
			level.get_children()[i].can_sleep = false


func _on_trigger_body_entered(body):
	if body == player:
		\"\"\"
		$Enemy/Info.disabled = false
		$Enemy2/Info.disabled = false
		\"\"\"
		var newenemy1 = enemy_type.instantiate()
		self.add_child(newenemy1)
		newenemy1.global_position = enemy1.global_position
		
		var newenemy2 = enemy_type.instantiate()
		self.add_child(newenemy2)
		newenemy2.global_position = enemy2.global_position
		
		$Player.sendHintToPlayer(\"[wave][center][color=SILVER]Navigate back to the start with the objective item\")
		
"

